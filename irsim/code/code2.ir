FUNCTION fact :
PARAM v1
IF v1 == #1 GOTO LABEL1
GOTO LABEL2
LABEL LABEL1 :
RETURN v1
GOTO LABEL3
LABEL LABEL2 :
t3 := v1 - #1
ARG t3
t2 := CALL fact
t1 := v1 * t2
RETURN t1
LABEL LABEL3 :
FUNCTION main :
READ v2
IF v2 > #1 GOTO LABEL4
GOTO LABEL5
LABEL LABEL4 :
ARG v2
v3 := CALL fact
GOTO LABEL6
LABEL LABEL5 :
v3 := #1
LABEL LABEL6 :
WRITE v3
RETURN #0